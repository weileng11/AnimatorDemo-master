		</activity>
		<activity android:name=".anim.Anim1Act"/>
		<activity android:name=".anim.Anim2Act"/>
		<activity android:name=".anim.Anim3Act"/>
		<activity android:name=".anim.Anim4Act"/>
		<activity android:name=".anim.Anim5Act"/>
		<activity android:name=".anim.Anim6Act"/>
	</application>
</manifest>